<Window x:Class="FeatureMatching.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Feature Matching" Height="350" Width="350"
        WindowStartupLocation="Manual" ResizeMode="NoResize"
        Left="40" Top="312" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,0,10,0">
            <Label Content="Frames: "/>
            <Label x:Name="labelFrameCounter"/>
        </StackPanel>
        <Button Content="Button" HorizontalAlignment="Left" Margin="126,112,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
